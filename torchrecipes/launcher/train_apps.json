{
    "train_apps":[
        {
            "name":"video_classification",
            "fbpkg":"pytorch_recipes_vision:1",
            "args":{
                "--config-name":"video_classification_train_app_conf",
                "--config-path":"../vision/video_classification/conf",
                "--package":"torchrecipes.vision.video_classification.conf"
            },
            "envs":{
                "CONFIG_MODULE":"torchrecipes.vision.video_classification.conf",
                "ENV":"prod"
            }
        },
        {
            "name":"doc_classification",
            "fbpkg":"pytorch_recipes_text:1",
            "args":{
                "--config-name":"doc_classification_app",
                "--config-path":"../text/conf",
                "--package":"torchrecipes.text.conf"
            },
            "envs":{
                "CONFIG_MODULE":"torchrecipes.text.conf",
                "ENV":"prod"
            }
        },
        {
            "name":"gan",
            "fbpkg":"pytorch_recipes_vision:1",
            "args":{
                "--config-name":"gan_train_app_conf",
                "--config-path":"../vision/GAN/conf",
                "--package":"torchrecipes.vision.GAN.conf"
            },
            "envs":{
                "CONFIG_MODULE":"torchrecipes.vision.GAN.conf",
                "ENV":"prod"
            }
        },
        {
            "name":"eval_inference",
            "fbpkg":"pytorch_recipes_vision:1",
            "args":{
                "--config-name":"eval_inference_train_app_conf",
                "--config-path":"../vision/eval_inference/conf",
                "--package":"torchrecipes.vision.eval_inference.conf"
            },
            "envs":{
                "CONFIG_MODULE":"torchrecipes.vision.eval_inference.conf",
                "ENV":"prod"
            }
        }
    ]
